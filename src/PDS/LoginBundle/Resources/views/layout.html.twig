<!DOCTYPE html>
<html lang="fr">
  <head>
    <title>{% block title %}PDS{% endblock %}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="icon" href="{{ asset('icones/32/gymnast5.png') }}">
    {% stylesheets
      '@PDSLoginBundle/Resources/public/css/main.css'
      'css/*' %}
      <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
    {% endstylesheets %}
    <!--[if lte IE 9]>
      <link rel="stylesheet" href="{{ asset('css/ace-part2.min.css') }}" />
      <link rel="stylesheet" href="{{ asset('css/ace-ie.min.css') }}" />
    <![endif]-->
    <!--[if lte IE 8]>
      <script src="{{ asset('js/html5shiv.min.js') }}"></script>
      <script src="{{ asset('js/respond.min.js') }}"></script>
      <script src="{{ asset('js/excanvas.min.js') }}"></script>
    <![endif]-->
  </head>
  <body>
    {% block body %}
    {% endblock %}
    {% javascripts 'js/*' %}
      <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
  </body>
</html>
